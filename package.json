{
  "name": "matrix",
  "version": "2.0.0",
  "description": "Tiny, high performance utilities for performing 2/3D matrix calculations. Full unit test coverage, compatible with Node/CommonJS, AMD, and browser globals.",
  "main": "./index.js",
  "main:esnext": "./index.es2015.js",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcherny/matrix.git"
  },
  "keywords": [
    "matrix",
    "flip",
    "identity",
    "css",
    "css3",
    "transform",
    "translate",
    "rotate",
    "skew",
    "perspective",
    "scale"
  ],
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "homepage": "https://github.com/bcherny/matrix#readme",
  "bugs": {
    "url": "https://github.com/bcherny/matrix/issues"
  },
  "scripts": {
    "build": "tsc -d -m es2015 --outDir .; mv ./index.js ./index.es2015.js; tsc -t es5 -m commonjs --outDir .;",
    "watch": "tsc -w",
    "test": "ava",
    "tdd": "npm run watch & npm test -- --watch"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "typescript": "^2.0.0"
  },
  "testling": {
    "files": "test/test.js",
